
        static boolean otherBinarySearch (int [] arr, int x) {
        int l = 0;
        int r = arr.length -1;

        while (r >= l){
            int m = l+(r-l) /2;
            if (arr[m] == x)
                 return true;
            if (arr[m] < x)
                    l=m+1;
            else
                    r=m-1;
        }
        return false;

    }
